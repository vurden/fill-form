{
  "name": "fill form",
  "description": "Save forms to fill later.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
      "48": "icon48px.png",
      "128": "icon128px.png"
  },
  "permissions": ["activeTab", "scripting", "tabs", "storage"],
  "action": {
    "default_title": "Click Me",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["background.js"]
    }
  ]
}
